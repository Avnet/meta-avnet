/include/ "system-conf.dtsi"

#include "include/dt-bindings/input/input.h"
#include "include/dt-bindings/gpio/gpio.h"

/ {
   model = "Avnet AUBoard 15P (MicroBlaze)";

   leds {
      compatible = "gpio-leds";
      pinctrl-names = "default";
      status = "okay";
   };

   keys {
      compatible = "gpio-keys";
      pinctrl-names = "default";
      status = "okay";
   };
};

&axi_iic_0 {
    status = "okay";
    pinctrl-names = "default";

    eeprom: eeprom@50 {
        compatible = "24c128";
        reg = <0x50>;
        pagesize = <64>;
    };
};
